<!DOCTYPE html>
<html>
<head>
  <title>tilblr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'header/header' %>


<section id="main">
  <%= yield %>
</section>

<% if logged_in? %>
  <%= render 'blogs/form' %>
<% end %>

</body>
</html>
