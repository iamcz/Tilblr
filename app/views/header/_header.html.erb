<header id="main-header">
  <navbar class="group">
    <h1 id="header-logo">
      <a href="#">til</a>
    </h1>
    <input class="searchbar" type="text" name="search_term" val="" placeholder="Search Tilblr">
    <% unless logged_in? %>
      <%= link_to "Log in", {controller: :sessions, action: :new}, { class: "login-link" } %>
      <%= link_to "Sign Up", {controller: :users, action: :new}, { class: "register-link" } %>
    <% else %>
      <%= render 'header/profile' %>
    <% end %>
  </navbar>
</header>
