<a href="/blogs/<%= blog.id %>" class="group">
  <figure class="blog-avatar">
    <img src="<%= blog.get('avatar_url') %>">
  </figure>

  <section class="blog-info group">
    <section class="text-info-section">
      <h6><%= blog.get("title") %></h6>
    </section>
    
    <section class="follow-button-section" %>
    </section>
  </section>
</a>
